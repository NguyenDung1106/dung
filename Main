		if v.Name == "Ice Admiral [Lv. 700] [Boss]" then

		else
			table.insert(BossTable, v.Name)
		end
	end
end


local Boss_Dropdown = Tab8:Dropdown("Select Boss",BossTable, function(value)
    _G.Settings.Boss["Select Boss"] = value
    SaveSettings()
end)
Tab8:Button("Refresh Boss", function(value)
    Boss_Dropdown:Clear();
	local BossTable = {}   
	for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
		if string.find(v.Name, "Boss") then
			if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
			else
                Boss_Dropdown:Add(v.Name)
			end
		end
	end
end)
Tab8:Label("Boss Config")
Tab8:Toggle("Auto Quest",false, function(value)
    _G.Settings.Boss["Auto Quest"] = value
    SaveSettings()
end)





















